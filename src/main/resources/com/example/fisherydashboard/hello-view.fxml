<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.fisherydashboard.HelloController">
    <HBox spacing="5.0" VBox.vgrow="ALWAYS">
        <ListView fx:id="lvCountry" prefHeight="400.0" prefWidth="117.0" />
      <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <VBox prefHeight="44.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="40.0" prefWidth="202.0">
                     <children>
                        <Label fx:id="lblCountryTitleText" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="124.0" prefWidth="100.0" spacing="5.0">
               <children>
                  <Label text="Total weight harvested in each region:">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <Label text="North East:" />
                        <Label fx:id="lblC1NE" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="North West:" />
                        <Label fx:id="lblC1NW" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="South East:" />
                        <Label fx:id="lblC1SE" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="203.0">
                     <children>
                        <Label text="South West:" />
                        <Label fx:id="lblC1SW" />
                     </children>
                  </HBox>
                  <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="62.0" prefWidth="100.0" />
               </children>
            </VBox>
            <VBox spacing="5.0">
               <children>
                  <Label text="Total weight harvested per average threshold:">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <Label fx:id="lblC4TotalWeightTH" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
      <VBox layoutX="263.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <VBox prefHeight="44.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="40.0" prefWidth="202.0" />
               </children>
            </VBox>
            <VBox prefHeight="124.0" prefWidth="100.0" spacing="5.0">
               <children>
                  <Label text="Most harvested fish specie:">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <PieChart fx:id="pieChart" prefWidth="168.0" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox spacing="5.0">
               <children>
                  <Label text="Total fish harvested per regional threshold:">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Label>
                  <HBox>
                     <children>
                        <Label text="North East:" />
                        <Label fx:id="lblC5NE" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="North West:" />
                        <Label fx:id="lblC5NW" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="South East:" />
                        <Label fx:id="lblC5SE" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="203.0">
                     <children>
                        <Label text="South West:" />
                        <Label fx:id="lblC5SW" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </VBox>
      <VBox layoutX="368.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
         <children>
            <VBox prefHeight="44.0" prefWidth="100.0">
               <children>
                  <HBox prefHeight="40.0" prefWidth="202.0" />
               </children>
            </VBox>
            <VBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="170.0" prefWidth="100.0" spacing="5.0" VBox.vgrow="ALWAYS">
               <children>
                  <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="16.0" prefWidth="100.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Amount of ">
                           <font>
                              <Font name="System Bold Italic" size="12.0" />
                           </font>
                        </Label>
                        <Label fx:id="lblC3FishName" />
                        <Label layoutX="10.0" layoutY="10.0" text=" harvested per region">
                           <font>
                              <Font name="System Bold Italic" size="12.0" />
                           </font>
                        </Label>
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="North East:" />
                        <Label fx:id="lblC3NE" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="North West:" />
                        <Label fx:id="lblC3NW" />
                     </children>
                  </HBox>
                  <HBox>
                     <children>
                        <Label text="South East:" />
                        <Label fx:id="lblC3SE" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="203.0">
                     <children>
                        <Label text="South West:" />
                        <Label fx:id="lblC3SW" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox spacing="5.0">
               <children>
                  <Label text="Most fish harvested by following members:">
                     <font>
                        <Font name="System Bold Italic" size="12.0" />
                     </font>
                  </Label>
                  <ListView fx:id="lvTop5Members" prefHeight="150.0" prefWidth="300" maxWidth="300"/>
               </children>
            </VBox>
         </children>
      </VBox>
    </HBox>
</VBox>
